@model IEnumerable<MiBotica.SolPedido.Entidades.Core.Servicio>

@{
    ViewBag.Title = "VerSoli";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SOLICITUDES PARA VALIDAR</h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<div class="box">
    <div class="box-body table-responsive no-padding">
        <table class="table">
            <tr>
                @*<th>
                    @Html.DisplayNameFor(model => model.IdServicio)
                </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.NEstudiante)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AEstudiante)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.dniEst)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RazonI)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CodEst)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IdEstudiante)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EstadoS)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FechaInicio)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    @*<td>
                            @Html.DisplayFor(modelItem => item.IdServicio)
                    </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.NEstudiante)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AEstudiante)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.dniEst)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.RazonI)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CodEst)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IdEstudiante)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EstadoS)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FechaInicio)
                    </td>
                    <td>
                        @Html.ActionLink("VALIDAR", "Validar", new { id = item.IdServicio })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>